@model OnlineTicariOtomasyon.Models.Siniflar.Departman
@{
    ViewBag.Title = "DepartmanEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yeni Departman Sayfası</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.DepartmanAdı)
    @Html.TextBoxFor(x => x.DepartmanAdı, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>
